lista = []
for x in range(3):
    for y in range(3):
        lista.append((x,y))
        
print(lista)

#lado esquerdo Ã© usado para mapeamento

lista2 = [
    (x, y)
    for x in range(3)
    for y in range(3)
        
]

lista3 = [
    
    [(x, letra) for letra in 'Breno']
    for x in range(3)
        
]

print(lista2)
print(lista3)